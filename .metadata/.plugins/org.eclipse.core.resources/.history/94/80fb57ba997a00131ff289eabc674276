package com.au.dynamicflame.domain;

import java.sql.Timestamp;

/**
 * News.java - Domain object representing the News table in the dynamic flame database. Will hold the details about 1
 * news article including the id's of any related images.
 * 
 * @author Alasdair
 * @since 11/01/2014
 */
public class News {

    String title;
    String subtitle;
    String content;
    String author;
    Timestamp posted_date;
    short image_id;
    byte likes;

    /**
     * @return the title
     */
    public String getTitle() {
        return title;
    }

    /**
     * @param p_title the title to set
     */
    public void setTitle(String p_title) {
        title = p_title;
    }

    /**
     * @return the subtitle
     */
    public String getSubtitle() {
        return subtitle;
    }

    /**
     * @param p_subtitle the subtitle to set
     */
    public void setSubtitle(String p_subtitle) {
        subtitle = p_subtitle;
    }

    /**
     * @return the content
     */
    public String getContent() {
        return content;
    }

    /**
     * @param p_content the content to set
     */
    public void setContent(String p_content) {
        content = p_content;
    }

    /**
     * @return the author
     */
    public String getAuthor() {
        return author;
    }

    /**
     * @param p_author the author to set
     */
    public void setAuthor(String p_author) {
        author = p_author;
    }

    /**
     * @return the posted_date
     */
    public Timestamp getPosted_date() {
        return posted_date;
    }

    /**
     * @param p_posted_date the posted_date to set
     */
    public void setPosted_date(Timestamp p_posted_date) {
        posted_date = p_posted_date;
    }

    /**
     * @return the image_id
     */
    public short getImage_id() {
        return image_id;
    }

    /**
     * @param p_image_id the image_id to set
     */
    public void setImage_id(short p_image_id) {
        image_id = p_image_id;
    }

    /**
     * @return the likes
     */
    public byte getLikes() {
        return likes;
    }

    /**
     * @param p_likes the likes to set
     */
    public void setLikes(byte p_likes) {
        likes = p_likes;
    }
}
